#Setup for the query tests. 

--let $no_of_runs = 10

# Checked that the performance_schema is setup correctly.;


--disable_query_log
--disable_result_log

UPDATE performance_schema.setup_instruments
       SET ENABLED = 'YES', TIMED = 'YES'
       WHERE NAME LIKE '%statement/%';
UPDATE performance_schema.setup_instruments
       SET ENABLED = 'YES', TIMED = 'YES'
       WHERE NAME LIKE '%stage/%';
UPDATE performance_schema.setup_consumers
       SET ENABLED = 'YES'
       WHERE NAME LIKE '%events_statements_%';
UPDATE performance_schema.setup_consumers
       SET ENABLED = 'YES'
       WHERE NAME LIKE '%events_stages_%';

SET GLOBAL general_log = 0;

--echo #
--echo #Setup the tables and import the standard data for queries to run
--source suite/histogram_plugin/include/setup_time_series_tables.include
--echo #--Tables setup
--source suite/histogram_plugin/include/populate_tables.include
--echo #--Base data imported

--enable_query_log
--enable_result_log

